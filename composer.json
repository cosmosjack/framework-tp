{
    "name": "oss_think/framework",
    "description": "This is the first original framework I want to make, modeled on TP. I want to make some upgrades on this",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "https://www.shikexu.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        },
        {
            "name": "yunwuxin",
            "email": "448901948@qq.com"
        },
        {
            "name": "jack",
            "email": "997823131@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "ext-mbstring": "*",
        "league/flysystem": "^1.1.4",
        "league/flysystem-cached-adapter": "^1.0",
        "psr/log": "~1.0",
        "psr/container": "~1.0",
        "psr/simple-cache": "^1.0",
        "oss_think/think-orm": "^2.0",
        "topthink/think-helper": "^3.1.1"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "think\\": "src/think/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "think\\tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
